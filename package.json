  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.91.0"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.1.0",
    "hono": "^4.6.0"
  }
}
